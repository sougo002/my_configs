[user]
	name = sougo
	email = "put your email here"
[init]
	defaultBranch = main
[alias]
	co = checkout
	cob = checkout -b
	com = commit
	ss = status
	br = branch
	fpush = push --force-with-lease --force-if-includes
	lo = log --oneline
	rh = reset --hard
	remove = reset HEAD~
[push]
	autoSetupRemote = true
[rebase]
	autostash = true
[core]
	editor = code --wait
[sequence]
	editor = code --wait
[ghq]
	root = ~/.ghq
